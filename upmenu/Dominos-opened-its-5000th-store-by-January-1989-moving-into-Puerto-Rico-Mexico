        :root {
            --initial-viewer-width: 640px;
            --initial-study-multiplier: 1;
            --initial-study-viewer-width: 25vw;
        }
            body {
                overflow-x: hidden;
            }
        </style>
        <script type="text/javascript">
        (function(){
            function getViewerWidth() {
                let vw = 100
                if (window.innerWidth >= 1199.99) {
                    vw = 75
                } else if (window.innerWidth >= 767.99) {
                    vw = 70
                }

                return Math.min(1076, (vw * window.innerWidth) / 100) + 32
            }

            function getViewerMultiplier() {
                if (window.innerWidth >= 1945 || window.innerWidth < 767) {
                    return 0
                }

                return 1
            }

            let _dvWidth = Math.min(1440, window.innerWidth)
            if(_dvWidth >= 1440) { // 80:20 ratio minus gap and horizontal padding
                _dvWidth = _dvWidth * 0.738
            } else if (_dvWidth >= 1200) { // Sidebar width is hardcoded so viewer begins to scale down
                _dvWidth = _dvWidth * 0.732
            } else if (_dvWidth >= 767) {
                _dvWidth = _dvWidth * 0.638
            } else {
                _dvWidth = _dvWidth - 32 // Viewer is full width minus horizontal padding
            }
            document.documentElement.style.setProperty('--initial-viewer-width', _dvWidth)
            document.documentElement.style.setProperty('--initial-study-multiplier', getViewerMultiplier())
            document.documentElement.style.setProperty('--initial-study-viewer-width', `${getViewerWidth()}px`)
        })()
        </script><style nonce="ttQfM0b_ThLgItt0N90hXs8511XgdbsXO-nIUkwmXfM" id="ssi-global5075218897-0">.ch-banner{margin-top:0 !important}.results-wrapper img{vertical-align:baseline !important}</style></head><body class="" data-theme="v2"><div id="block_ssi-global_HeaderFragment" style="z-index:1000;top:0px;position:sticky"><header id="ch-global-header" data-testid="ch-global-header" class="tw-relative tw-flex tw-justify-between tw-items-center tw-bg-white tw-py-4 tw-shadow tw-z-[1299] tw-px-8 md:tw-px-16"><div id="ssi-global-header-content-wrapper" class="tw-flex tw-flex-1 tw-items-center md:tw-w-1/2"><div data-headlessui-state=""><button aria-expanded="false" aria-label="Close main menu" class="tw-flex focus:tw-outline-none focus-visible:tw-ring-2 focus-visible:tw-ring-blue-50 tw-mr-8" type="button" data-headlessui-state=""><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true" class="tw-w-12 tw-h-12" id="mobileHamburgerIcon"><path fill-rule="evenodd" d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path></svg></button><div class="tw-absolute tw-pt-14 tw-mt-9 tw-z-[1071] tw-w-full tw-bg-white tw-left-0" hidden="" style="display:none" tabindex="-1" data-headlessui-state=""><div class="tw-overflow-auto tw-relative" style="height:calc(100vh - 64px)"><div class="tw-flex tw-flex-col tw-gap-4 tw-mx-8"><button class="tw-text-xl tw-font-medium tw-flex tw-justify-center tw-w-full
